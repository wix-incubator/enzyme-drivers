{
  "name": "enzyme-drivers",
  "version": "3.0.0",
  "description": "testing utility lib",
  "main": "lib/index.js",
  "scripts": {
    "build": ":",
    "lint": "eslint src test",
    "pretest": "npm run lint",
    "test": "jest",
    "release": "wnpm-release -- --no-shrinkwrap",
    "postrelease": "echo \"##teamcity[buildStatus text='Version: `jq -r '.version' package.json`; {build.status.text}']\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/enzyme-drivers.git"
  },
  "keywords": [
    "enzyme",
    "testing",
    "unit",
    "test",
    "component",
    "test",
    "react",
    "react-native",
    "rn"
  ],
  "author": "Ofir Dagan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wix/enzyme-drivers/issues"
  },
  "homepage": "https://github.com/wix/enzyme-drivers#readme",
  "dependencies": {
    "create-react-class": "^15.6.2",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-test-renderer": "16.8.6"
  },
  "devDependencies": {
    "eslint": "^3.4.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-react": "^6.2.0",
    "jest-cli": "^24.8.0",
    "jest-teamcity-reporter": "^0.9.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.8.6",
    "react-native": "~0.59.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "preset": "react-native",
    "testMatch": [
      "**/?(*.)(spec|test|it).(ts|tsx|js|jsx)"
    ],
    "testPathIgnorePatterns": [
      "test",
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-teamcity-reporter",
    "setupFilesAfterEnv": [
      "<rootDir>/test/spec/support/jest-setup-framework.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native-community|wix-one-app-engine|react-native-testing-library|wix-social-chat-mobile)/)"
    ],
    "globals": {
      "preset": "react-native"
    }
  },
  "optionalDependencies": {
    "wnpm-ci": "^6.1.166"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
